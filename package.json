{
   "name": "yaml-tempfile",
   "version": "0.1.0",
   "description": "Yaml模板自动化生成文件，批量管理自动化生成文件, 支持vue/react，Generate files by yaml config and templates, plugin for vue/react",
   "author": "Alex.Hua",
   "license": "MIT",
   "main": "./dist/index.js",
   "bin": {
      "yaml-tempfile": "./dist/cli.js"
   },
   "scripts": {
      "build": "rm -rf dist && rollup --bundleConfigAsCjs -c rollup.config.js",
      "test": "rm -rf dist && rollup --bundleConfigAsCjs -c rollup.config.js && bin/cli.js -c samples/config.yaml"
   },
   "exports": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
   },
   "dependencies": {
      "commander": "^11.0.0",
      "consola": "^3.2.3",
      "ejs": "^3.1.9",
      "js-yaml": "^4.1.0",
      "jsonpath": "^1.1.1",
      "merge-anything": "^5.1.7"
   },
   "devDependencies": {
      "@types/js-yaml": "^4.0.5",
      "@types/node": "^20.6.0",
      "@typescript-eslint/eslint-plugin": "^6.7.0",
      "@typescript-eslint/parser": "^6.7.0",
      "eslint": "^8.49.0",
      "eslint-scope": "^7.2.2",
      "eslint-visitor-keys": "^3.4.3",
      "rollup": "^3.29.2",
      "rollup-plugin-delete": "^2.0.0",
      "rollup-plugin-typescript2": "^0.35.0",
      "tslib": "^2.6.2",
      "typescript": "^5.2.2"
   }
}
